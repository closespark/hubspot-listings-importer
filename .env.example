# Example environment variables
# Copy this file to .env for local development

# Required: HubSpot Private App Access Token
HUBSPOT_ACCESS_TOKEN=your_hubspot_access_token_here

# Required: URL to the JSON feed containing real estate listings
FEED_URL=https://example.com/api/listings.json

# Optional: Log level (error, warn, info, debug)
LOG_LEVEL=info

# Optional: Number of retry attempts for API calls (default: 3)
RETRY_ATTEMPTS=3

# Optional: Initial retry delay in milliseconds (default: 1000)
RETRY_DELAY=1000

# Optional: Batch size for processing listings (default: 100)
BATCH_SIZE=100
